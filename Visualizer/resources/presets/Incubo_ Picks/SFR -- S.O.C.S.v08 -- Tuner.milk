per_frame_1=//           SOCS -- Standard Operating Control System
per_frame_2=//                      By: Stirfriedramen
per_frame_3=//------------------------------------------------------------------
per_frame_4=//  This code provides a broader array of signals for 
per_frame_5=// controlling the preset that you write on top of the SOCS.  
per_frame_6=// It is not meant to replace the bass, mid, treb & _att 
per_frame_7=// signals, but to work along side them.
per_frame_8=//------------------------------------------------------------------
per_frame_9=// OUTPUTS:
per_frame_10=//q32 -- volume        Rough signal indicating loudness (0 to 2-ish)
per_frame_11=//q31 -- volume_att    Smooth loudness signal (0 to 2-ish)
per_frame_12=//q30 -- bass_beat     boolian (1 or 0) 1 = sudden spike in bass
per_frame_13=//q29 -- bass_weight   % of total volume that is bass  
per_frame_14=//q28 -- mid_beat      boolian (1 or 0) 1 = sudden spike in mid
per_frame_15=//q27 -- mid_weight    % of total volume that is mid  
per_frame_16=//q26 -- treb_beat     boolian (1 or 0) 1 = sudden spike in treb
per_frame_17=//q25 -- treb_weight   % of total volume that is treb
per_frame_18=//------------------------------------------------------------------
per_frame_19=// CONTROLS:
per_frame_20=//   set these variables to your taste, (range is 0 to 1)
per_frame_21=// they set the senstivity of the beat detectors
per_frame_22=b_beat_sens = .4;
per_frame_23=m_beat_sens = .4;
per_frame_24=t_beat_sens = .45;
per_frame_25=// set weights_smooth to 0 if you want to use the rougher signals
per_frame_26=weights_smooth = 0;
per_frame_27=//------------------------------------------------------------------
per_frame_28=// SOCS Code:
per_frame_29=q32 = 1 / (bass + mid + treb);
per_frame_30=q31 = 1 / (bass_att + mid_att + treb_att);
per_frame_31=q30 = if((floor(max(bass - bass_att, 0) * (20 * b_beat_sens))),1,0);
per_frame_32=q29 = if((weights_smooth), bass_att * q31, bass * q32);
per_frame_33=q28 = if((floor(max(mid  -  mid_att, 0) * (20 * m_beat_sens))),1,0);
per_frame_34=q27 = if((weights_smooth), mid_att * q31, mid * q32);
per_frame_35=q26 = if((floor(max(treb - treb_att, 0) * (20 * t_beat_sens))),1,0);
per_frame_36=q25 = if((weights_smooth), treb_att * q31, treb * q32);
per_frame_37=q31 = (bass_att + mid_att + treb_att) * 0.060606;
per_frame_38=q32 = (bass + mid + treb) * 0.060606;
per_frame_39=//------------------------------------------------------------------
per_frame_40=//
per_frame_41=// You should still have 28 user variables left and q1 to q24 
per_frame_42=//
per_frame_43=// Of course you can overwrite any of the q variables that you aren't
per_frame_44=// using in your preset. 
per_frame_45=//------------------------------------------------------------------
per_frame_46=//     VVVVVVVVVVV  Add your beautiful code below  VVVVVVVVVVV
per_frame_47=//------------------vvvvvvvvvvvvvvvvvvvvvvvvvvvvv-------------------
per_frame_48=
per_frame_49=q1 = if((weight_smooth),min(treb_att,1),min(treb,1));
per_frame_50=q2 = if((weight_smooth),min(mid_att,1),min(mid,1));
per_frame_51=q3 = if((weight_smooth),min(bass_att,1),min(bass,1));
per_frame_52=q4 = weights_smooth;
warp_1=`shader_body
warp_2=`{
warp_3=`    // sample previous frame
warp_4=`    ret = tex2D( sampler_main, uv ).xyz;
warp_5=`    
warp_6=`    // darken (decay) over time
warp_7=`    ret *= 0.84; //or try: ret -= 0.004;
warp_8=`}
comp_1=`sampler sampler_smooth;
comp_2=`sampler sampler_raw;
comp_3=`shader_body
comp_4=`{
comp_5=`    float3 fore = tex2D(sampler_main, uv).xyz;
comp_6=`    float3 back_s = tex2d(sampler_smooth, uv);
comp_7=`    float3 back_r = tex2d(sampler_raw, uv);
comp_8=`    ret = fore + (back_s * q4) + (back_r * (1-q4));
comp_9=`    ret *= 1.00; //gamma
comp_10=`}
