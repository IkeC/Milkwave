comp_231=shader_body {
comp_232=  float4 fragColor = 0;
comp_233=  // CONV: Center on screen, then try some aspect correction
comp_234=  uv = (uv*2) - 1;
comp_235=  uv *= aspect.xy;
comp_236=  // CONV: Adjust this to flip the output (±uv.x, ±uv.y)
comp_237=  uv = float2(uv.x, -uv.y);
comp_238=  float2 fragCoord  = uv;
comp_239=
comp_240=  // uv = fragCoord.xy / uv.xy*2.-1.;
comp_241=  float2 oriuv=uv;
comp_242=  // uv.y*=uv.y/uv.x;
comp_243=
comp_244=  // float2 M=(mouse.xy/uv.xy-.5)*3.;
comp_245=  float2 M = float2(-mouse.x+0.5, mouse.y-0.5)*1.5;
comp_246=  if (mouse.z<1.) M=float2(0.,-0.05);
comp_247=
comp_248=  // "zoom"
comp_249=  float fov=.9-max(0.,.7-time*.3);
comp_250=  float3 dir=normalize(float3(uv*fov,1.));
comp_251=
comp_252=  dir.yz=mul(dir.yz,rot(M.y));
comp_253=  dir.xz=mul(dir.xz,rot(M.x));
comp_254=
comp_255=  float3 origin=float3(-1.,.7,0.);
comp_256=  float3 from=origin+move(dir);
comp_257=
comp_258=  from.x += 2;
comp_259=  from.y += 0.05;
comp_260=
comp_261=  // from.x += -1.1;
comp_262=  // from.y += -0.9;
comp_263=
comp_264=  float3 color=raymarch(from,dir);
comp_265=  #ifdef BORDER
comp_266=  color=lerp(float3(0., 0., 0.),color,pow(max(0.,.95-length(oriuv*oriuv*oriuv*float2(1.05,1.1))),.3));
comp_267=  #endif
comp_268=  fragColor = float4(color,1.);
comp_269=  ret = fragColor;
comp_270=}
