comp_5=`#define hash(x) frac(sin(x) * 43758.5453123)
comp_6=`float3 pal(float t){return .5+.5*cos(6.28*(1.*t+float3(.0,.1,.1)));}
comp_7=` float stepNoise(float x, float n) { // From Kamoshika shader
comp_8=`   float factor = 0.3;
comp_9=`   float i = floor(x);
comp_10=`   float f = x - i;
comp_11=`   float u = smoothstep(0.5 - factor, 0.5 + factor, f);
comp_12=`   float res = lerp(floor(hash(i) * n), floor(hash(i + 1.) * n), u);
comp_13=`   res /= (n - 1.) * 0.5;
comp_14=`   return res - 1.;
comp_15=` }
comp_16=` float3 path(float3 p){
comp_17=`   
comp_18=`      float3 o = float3(0,0,0);
comp_19=`       o.x += stepNoise(p.z*.05,5.)*5.;
comp_20=`      o.y += stepNoise(p.z*.07,3.975)*5.;
comp_21=`     return o;
comp_22=`   }
comp_23=`   float diam2(float2 p,float s){p=abs(p); return (p.x+p.y-s)*rsqrt(3.);}
comp_24=`   float3 erot(float3 p,float3 ax,float t){return lerp(dot(ax,p)*ax,p,cos(t))+cross(ax,p)*sin(t);}
comp_25=`shader_body
comp_26=`{
comp_27=`    uv = uv * 2 - 1;
comp_28=`    uv  *= aspect.xy;
comp_29=`
comp_30=`    float3 col = float3(0,0,0);
comp_31=`  
comp_32=`  float3 ro = float3(0.,0.,-1.),rt=float3(0,0,0);
comp_33=`  ro.z+=time*5.;
comp_34=`  rt.z += time*5.;
comp_35=`  ro+=path(ro);
comp_36=`    rt+=path(rt);
comp_37=`  float3 z = normalize(rt-ro);
comp_38=`  float3 x = float3(z.z,0.,-z.x);
comp_39=`  float i=0.,e=0.,g=0.;
comp_40=`  float3 rd = mul(erot(normalize(float3(uv, 1.)), float3(0., 0., 1.), stepNoise(time + hash(uv.x * uv.y * time) * .05, 6.)), 
comp_41=`              float3x3(x, cross(z, x), z)); //DeepSeek - Corrected the matrix order
comp_42=`  /* Matrix order types for different shaders:
comp_43=`  GLSL - column-major order
comp_44=`  HLSL - row-major order
comp_45=`  */
comp_46=`  for(;i++<99.;){
