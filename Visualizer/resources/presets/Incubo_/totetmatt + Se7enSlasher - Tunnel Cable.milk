comp_48=`
comp_49=`    p-=path(p);
comp_50=`    float r = 0.;;
comp_51=`    float3 pp=p;
comp_52=`    float sc=1.;
comp_53=`    for(float j=0.;j++<4.;){
comp_54=`        r = clamp(r+abs(dot(sin(pp*3.),cos(pp.yzx*2.))*.3-.1)/sc,-.5,.5);
comp_55=`        pp=erot(pp,normalize(float3(.1,.2,.3)),.785+j);
comp_56=`        pp+=pp.yzx+j*50.;
comp_57=`        sc*=1.5;
comp_58=`        pp*=1.5;
comp_59=`      }
comp_60=`      
comp_61=`     float h = abs(diam2(p.xy,7.))-3.-r;
comp_62=`   
comp_63=`     p=erot(p,float3(0.,0.,1.),path(p).x*.5+p.z*.2);
comp_64=`    float t = length(abs(p.xy)-.5)-.1;
comp_65=`     h= min(t,h);
comp_66=`     g+=e=max(.001,t==h ?abs(h):(h));
comp_67=`     col +=(t==h ?float3(.3,.2,.1)*(100.*exp(-20.*frac(p.z*.25+time)))*fmod(floor(p.z*4.)+fmod(floor(p.y*4.),2.),2.) :float3(.1,.1,.1))*.0325/exp(i*i*e);;
comp_68=`    }
