per_frame_1=//          3 SOCS.20 -- Standard Operating Control System
per_frame_2=//                      By: Stirfriedramen
per_frame_3=// OUTPUTS:-------------------------------------------------------------------------------------------
per_frame_4=//q32 -- volume        Rough signal indicating loudness (0 to 2-ish)
per_frame_5=//q31 -- volume_att    Attenuated loudness signal       (0 to 2-ish)
per_frame_6=//q30 -- vol_texture   How rough or smooth is volume (0=leg, 1=stec)
per_frame_7=//q29 -- bass_beat     boolian (1 or 0) 1 = sudden spike in bass
per_frame_8=//q28 -- bass_weight   % of total volume that is bass (0 to 1)
per_frame_9=//q27 -- bass_text     How rough or smooth is bass   (0=leg, 1=stec) 
per_frame_10=//q26 -- bass_swell    How fast is the bass signal strengthening
per_frame_11=//q25 -- bass_presence How bassy is the overall sound
per_frame_12=//q24 -- mid_beat      boolian (1 or 0) 1 = sudden spike in mid
per_frame_13=//q23 -- mid_weight    % of total volume that is mid  (0 to 1)
per_frame_14=//q22 -- mid_text      How rough or smooth is mid    (0=leg, 1=stec) 
per_frame_15=//q21 -- nid_swell     How fast is the mid signal rising
per_frame_16=//q20 -- mid_presence  How mid is the sound overall
per_frame_17=//q19 -- treb_beat     boolian (1 or 0) 1 = sudden spike in treb
per_frame_18=//q18 -- treb_weight   % of total volume that is treb (0 to 1)
per_frame_19=//q17 -- treb_tex      How rough or smooth is treb   (0=leg, 1=stec) 
per_frame_20=//q16 -- treb_swell    How fast is the treb signal rising
per_frame_21=//q15 -- treb_presence How Treb is the sound overall
per_frame_22=//q14 -- tone          How high or low is the sound (-1 = bass - +1 = treb)
per_frame_23=//q13 -- swell         How fast is the volume increasing
per_frame_24=
per_frame_25=// CONTROLS:-----------------------------------------------------------------------------------------
per_frame_26=//   set these variables to taste, or control them with your code!
per_frame_27=
per_frame_28=// These increase the senstivity of the beat detectors (0 to 1)
per_frame_29=b_beat_sens = .5;    
per_frame_30=m_beat_sens = .5;    
per_frame_31=t_beat_sens = .5;
per_frame_32=
per_frame_33=// Use these to reduce the sensitivity of the texture signals (0 .. 100)
per_frame_34=v_tex_sens = 5.00;    
per_frame_35=b_tex_sens = 5.00;    
per_frame_36=m_tex_sens = 5.00;    
per_frame_37=t_tex_sens = 5.00;
per_frame_38=
per_frame_39=// Set weights_smooth to 0 if you want to use the rougher signal
per_frame_40=weights_smooth = 1;
per_frame_41=
per_frame_42=// SOCS Code:----------------------------------------------------------------------------------------
per_frame_43=q32 = 1 / min((bass + mid + treb), 33);    q31 = 1 / min((bass_att + mid_att + treb_att), 33);
per_frame_44=q29 = if((floor(max(bass - bass_att, 0) * (20 * b_beat_sens))),1,0);
per_frame_45=q28 = if((weights_smooth), bass_att * q31, bass * q32);
per_frame_46=b_tex = (b_tex * (100 - b_tex_sens) + abs(bass - bass_att) * b_tex_sens) * 0.01;
per_frame_47=q27 = min(b_tex * 1.333, 1);    q25 = min(q28 * bass_att, 1.00);
per_frame_48=q26 = min(max(bass - bass_att, 0), 1);    q24 = if((floor(max(mid  -  mid_att, 0) * (20 * m_beat_sens))),1,0);
per_frame_49=q23 = if((weights_smooth), mid_att * q31, mid * q32);
per_frame_50=m_tex = (m_tex * (100 - m_tex_sens) + abs(mid  -  mid_att) * m_tex_sens) * 0.01;
per_frame_51=q22 = min(m_tex * 1.166, 1);    q20 = min(q23 * bass_att, 1.00);    
per_frame_52=q21 = min(max(mid - mid_att, 0), 1);
per_frame_53=q19 = if((floor(max(treb - treb_att, 0) * (20 * t_beat_sens))),1,0);
per_frame_54=q18 = if((weights_smooth), treb_att * q31, treb * q32);
per_frame_55=t_tex = (t_tex * (100 - t_tex_sens) + abs(treb - treb_att) * t_tex_sens) * 0.01;
per_frame_56=q17 = min(t_tex * 1.166, 1);    q15 = q18 * bass_att;
per_frame_57=q16 = min(max(treb - treb_att, 0), 1);    q14 = (q18 - q28);    q32 = min((bass + mid + treb)* 0.25, 2);
per_frame_58=q31 = min((bass_att + mid_att + treb_att) * 0.25, 2);     q13 = min(max((q32 - q31), 0), 1);
per_frame_59=v_tex = (v_tex * (100 - v_tex_sens) + abs(q32 - q31) * v_tex_sens) * 0.01;
per_frame_60=q30 = min(v_tex, 1);    //q31 *= 0.99;   //q32 *= 0.60606;   
