comp_1=`float rand(float n){return frac(sin(n) * 43758.5453123);}
comp_2=`float noise(float p){
comp_3=`    float fl = floor(p);
comp_4=`    float fc = frac(p);
comp_5=`    return lerp(rand(fl), rand(fl + 1.0), fc);
comp_6=`}
comp_7=`float blockyNoise(float2 uvx, float threshold, float scale, float seed) {
comp_8=`    float scroll = floor(time + sin(11.0 * time) + sin(time)) * 0.77;
comp_9=`    float2 noiseUV = uvx.yy / scale + scroll;
comp_10=`    float noise2 = tex2D(sampler_noise_lq, noiseUV).r;
comp_11=`    float id = floor(noise2 * 20.0);
comp_12=`    id = noise(id + seed) - 0.5;
comp_13=`    if (abs(id) > threshold) id = 0.0;
comp_14=`    return id;
comp_15=`}
comp_16=`shader_body {
comp_17=`    float rgbIntensity = 0.1 + 0.1 * sin(time * 3.7);
comp_18=`    float displaceIntensity = 0.2 + 0.3 * pow(sin(time * 1.2), 5.0);
comp_19=`    float interlaceIntensity = 0.01;
comp_20=`    float dropoutIntensity = 0.1;
comp_21=`    float displace = blockyNoise(uv + float2(uv.y, 0.0), displaceIntensity, 25.0, 66.6);
comp_22=`    displace *= blockyNoise(uv.yx + float2(0.0, uv.x), displaceIntensity, 111.0, 13.7);
comp_23=`    uv.x += displace;
comp_24=`    float2 offs = 0.1 * float2(blockyNoise(uv.xy + float2(uv.y, 0.0), rgbIntensity, 65.0, 341.0), 0.0);
comp_25=`    float3 col = tex2D(sampler_main, uv - offs).rgb;
comp_26=`    col.g = tex2D(sampler_main, uv).g;
comp_27=`    col.b = tex2D(sampler_main, uv + offs).b;
comp_28=`    float line = frac(uv.y * texsize.y / 3.0);
comp_29=`    float3 mask = float3(3.0, 0.0, 0.0);
comp_30=`    if (line > 0.333) mask = float3(0.0, 3.0, 0.0);
comp_31=`    if (line > 0.666) mask = float3(0.0, 0.0, 3.0);
comp_32=`    float maskNoise = blockyNoise(uv, interlaceIntensity, 90.0, time) * max(displace, offs.x);
comp_33=`    maskNoise = 1.0 - maskNoise;
comp_34=`    if (maskNoise == 1.0) mask = float3(1, 1, 1);
comp_35=`    float dropout = blockyNoise(uv, dropoutIntensity, 11.0, time) * blockyNoise(uv.yx, dropoutIntensity, 90.0, time);
comp_36=`    mask *= (1.0 - 5.0 * dropout);
comp_37=`    ret = float4(mask * col, 1.0);
comp_38=`}
