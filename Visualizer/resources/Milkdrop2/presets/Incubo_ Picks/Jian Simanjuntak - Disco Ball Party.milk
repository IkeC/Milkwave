warp_11=`    //float2 st = (5.* uv-2 - uv.xy) / max(uv.x-.5, uv.y+2.5);
warp_12=`    float2 st = uv * q5 - q9;
warp_13=`    st *= 1.0 - 0.2 * cos (st.yx) * sin (3.14159 * 0.5 * q2);
warp_14=`    st *= 5.0;
warp_15=`    float random = rand (floor (st));
warp_16=`    float2 black = smoothstep (1.0, 0.8, cos (st * 3.14159 * 2.0));
warp_17=`    float3 color = hsv2rgb (float3 (random, 1.0, 1.0));
warp_18=`    color *= black.x * black.y * smoothstep (1.0, 0.0, length (frac (st) - 0.5));
warp_19=`    color *= 0.5 + 0.5 * cos (random + random * time + time + 3.14159 * 0.5 * q2);
warp_20=`    ret = float4 (color, q2);
warp_21=`}
