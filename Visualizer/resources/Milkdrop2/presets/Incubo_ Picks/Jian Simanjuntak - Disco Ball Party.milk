per_frame_18=v = 0.1;
per_frame_19=j1 = j1*0.95 + sqr(bass*4)*v;
per_frame_20=j2 = j2*0.95 + sqr(mid*4)*v;
per_frame_21=j3 = j3*0.95 + sqr(treb*4)*v;
per_frame_22=
per_frame_23=n = n + j1*0.0052;
per_frame_24=n1 = n1 + j2*0.0052;
per_frame_25=n2 = n2 + j3*0.0052;
per_frame_26=
per_frame_27=
per_frame_28=//w1 = n*0.01;    // solid angles to rotate the sphere in its origin
per_frame_29=w2 = n1*0.01;
per_frame_30=w3 = n2*0.01;
per_frame_31=vol = 0.1*(vol*9 + (bass_att+mid_att+treb_att)*0.333333);
per_frame_32=
per_frame_33=mtime = mtime + vol*0.6;
per_frame_34=q2=mtime*0.09; // Fast moving ball, warp shader, and kaleidoscope control //
per_frame_35=r1 = 0.1+0.1*sin(mtime*0.0);
per_frame_36=w1 = r1;
per_frame_37=w3 = 0.1;
per_frame_38=w2 = mtime*0.1;
per_frame_39=wc = mtime*0.02;
per_frame_40=q10=w2;
per_frame_41=q13=sin(w2);
per_frame_42=q14 = sin(w2*0.8);
