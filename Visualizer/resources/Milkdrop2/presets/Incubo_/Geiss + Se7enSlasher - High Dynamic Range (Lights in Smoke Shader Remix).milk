comp_24=`float snoise(float3 v)
comp_25=`  { 
comp_26=`  const float2  C = float2(1.0/6.0, 1.0/3.0) ;
comp_27=`  const float4  D = float4(0.0, 0.5, 1.0, 2.0);
comp_28=`
comp_29=`// First corner
comp_30=`  float3 i  = floor(v + dot(v, C.yyy) );
comp_31=`  float3 x0 =   v - i + dot(i, C.xxx) ;
comp_32=`
comp_33=`// Other corners
comp_34=`  float3 g = step(x0.yzx, x0.xyz);
comp_35=`  float3 l = 1.0 - g;
comp_36=`  float3 i1 = min( g.xyz, l.zxy );
comp_37=`  float3 i2 = max( g.xyz, l.zxy );
comp_38=`
comp_39=`  //   x0 = x0 - 0.0 + 0.0 * C.xxx;
comp_40=`  //   x1 = x0 - i1  + 1.0 * C.xxx;
comp_41=`  //   x2 = x0 - i2  + 2.0 * C.xxx;
comp_42=`  //   x3 = x0 - 1.0 + 3.0 * C.xxx;
comp_43=`  float3 x1 = x0 - i1 + C.xxx;
comp_44=`  float3 x2 = x0 - i2 + C.yyy; // 2.0*C.x = 1/3 = C.y
comp_45=`  float3 x3 = x0 - D.yyy;      // -1.0+3.0*C.x = -0.5 = -D.y
comp_46=`
comp_47=`// Permutations
comp_48=`  i = mod289(i); 
comp_49=`  float4 p = permute( permute( permute( 
comp_50=`             i.z + float4(0.0, i1.z, i2.z, 1.0 ))
comp_51=`           + i.y + float4(0.0, i1.y, i2.y, 1.0 )) 
comp_52=`           + i.x + float4(0.0, i1.x, i2.x, 1.0 ));
comp_53=`
comp_54=`// Gradients: 7x7 points over a square, mapped onto an octahedron.
comp_55=`// The ring size 17*17 = 289 is close to a multiple of 49 (49*6 = 294)
comp_56=`  float n_ = 0.142857142857; // 1.0/7.0
comp_57=`  float3  ns = n_ * D.wyz - D.xzx;
comp_58=`
comp_59=`  float4 j = p - 49.0 * floor(p * ns.z * ns.z);  //  mod(p,7*7)
comp_60=`
comp_61=`  float4 x_ = floor(j * ns.z);
comp_62=`  float4 y_ = floor(j - 7.0 * x_ );    // mod(j,N)
comp_63=`
comp_64=`  float4 x = x_ *ns.x + ns.yyyy;
comp_65=`  float4 y = y_ *ns.x + ns.yyyy;
comp_66=`  float4 h = 1.0 - abs(x) - abs(y);
comp_67=`
comp_68=`  float4 b0 = float4( x.xy, y.xy );
comp_69=`  float4 b1 = float4( x.zw, y.zw );
comp_70=`
comp_71=`  //float4 s0 = float4(lessThan(b0,0.0))*2.0 - 1.0;
comp_72=`  //float4 s1 = float4(lessThan(b1,0.0))*2.0 - 1.0;
comp_73=`  float4 s0 = floor(b0)*2.0 + 1.0;
comp_74=`  float4 s1 = floor(b1)*2.0 + 1.0;
comp_75=`  float4 sh = -step(h, float4(0, 0, 0, 0));
comp_76=`
comp_77=`  float4 a0 = b0.xzyw + s0.xzyw*sh.xxyy ;
comp_78=`  float4 a1 = b1.xzyw + s1.xzyw*sh.zzww ;
comp_79=`
comp_80=`  float3 p0 = float3(a0.xy,h.x);
comp_81=`  float3 p1 = float3(a0.zw,h.y);
comp_82=`  float3 p2 = float3(a1.xy,h.z);
comp_83=`  float3 p3 = float3(a1.zw,h.w);
comp_84=`
comp_85=`//Normalise gradients
comp_86=`  float4 norm = taylorInvSqrt(float4(dot(p0,p0), dot(p1,p1), dot(p2, p2), dot(p3,p3)));
comp_87=`  p0 *= norm.x;
comp_88=`  p1 *= norm.y;
comp_89=`  p2 *= norm.z;
comp_90=`  p3 *= norm.w;
comp_91=`
comp_92=`// Mix final noise value
comp_93=`  float4 m = max(0.6 - float4(dot(x0,x0), dot(x1,x1), dot(x2,x2), dot(x3,x3)), 0.0);
comp_94=`  m = m * m;
comp_95=`  return 42.0 * dot( m*m, float4( dot(p0,x0), dot(p1,x1), 
comp_96=`                                dot(p2,x2), dot(p3,x3) ) );
comp_97=`  }
