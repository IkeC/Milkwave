comp_31=`float rad4 = q14;
comp_32=`
comp_33=`
comp_34=`float3 lens1 = figure_circle(base_uv,pos1,rad1);
comp_35=`float3 lens2 = figure_circle(base_uv,pos2,rad2);
comp_36=`float3 lens3 = figure_circle(base_uv,pos3,rad3);
comp_37=`float3 lens4 = figure_circle(base_uv,pos4,rad4);
comp_38=`
comp_39=`
comp_40=`
comp_41=`float2 lens_uv = uv_scale(base_uv,pos1,1-lens1*0.9);
comp_42=`       lens_uv = uv_scale(lens_uv,pos2,1-lens2*0.9);
comp_43=`       lens_uv = uv_scale(lens_uv,pos3,1-lens3*0.9);
comp_44=`       lens_uv = uv_scale(lens_uv,pos4,1-lens4*0.9);
comp_45=`
comp_46=`uv = lens_uv;
comp_47=`
comp_48=`    float2 d = texsize.zw*1.2;
comp_49=`    float3 dx = ( GetPixel(uv + float2(1,0)*d) - GetPixel(uv-float2(1,0)*d) );
comp_50=`    float3 dy = ( GetPixel(uv + float2(0,1)*d) - GetPixel(uv-float2(0,1)*d) );
comp_51=`
comp_52=`
comp_53=`
comp_54=`    float c = 4;
comp_55=`    float dd = 1;
comp_56=`
comp_57=`float3 light1 = dd - pow(length(uv-float2(lum(dx),lum(dy))*c-pos1),.2);
comp_58=`float3 light2 = dd - pow(length(uv-float2(lum(dx),lum(dy))*c-pos2),.2);
comp_59=`float3 light3 = dd - pow(length(uv-float2(lum(dx),lum(dy))*c-pos3),.2);
comp_60=`float3 light4 = dd - pow(length(uv-float2(lum(dx),lum(dy))*c-pos4),.2);
comp_61=`
comp_62=`
comp_63=`ret = lerp(GetPixel(uv),1,(max(light1,light2),max(light3,light4)));
comp_64=`
comp_65=`ret = lerp(ret,float3(2,1,-1),lens1*0.5);
comp_66=`ret = lerp(ret,float3(2,-1,1),lens2*0.5);
comp_67=`ret = lerp(ret,float3(-1,1,2),lens3*0.5);
comp_68=`ret = lerp(ret,float3(1,-1,2),lens4*0.5);
comp_69=`
comp_70=`}
