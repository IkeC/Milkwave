comp_46=`float4 track(float2 uv4){
comp_47=`    float b = .01;
comp_48=`    float trk = .0;
comp_49=`    float sup = .0;
comp_50=`    
comp_51=`    trk += st(-.005, .005, uv4.y + height(uv4.x) + .007);
comp_52=`    trk *= st(.005, -.005, uv4.y + height(uv4.x) - .007);
comp_53=`    
comp_54=`    float2 ruv = frac(uv4*8.) - .5;
comp_55=`    
comp_56=`    sup += st(.08, .07, abs(dot(ruv, float2(1., 1.))));
comp_57=`    sup += st(.08, .07, abs(dot(ruv, float2(-1., 1.))));
comp_58=`    sup *= st(.001, -.001, uv4.y + height(uv4.x)+.007);
comp_59=`    
comp_60=`    float3 col = float3(.7, .7, .7)*trk;
comp_61=`    
comp_62=`    return float4(col, trk + sup);
comp_63=`}
