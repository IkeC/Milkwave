comp_66=`float4 car(float2 uv5){
comp_67=`    float b = .02;
comp_68=`    
comp_69=`    float3 col = float3(0, 0, 0);
comp_70=`    float a = 0.;
comp_71=`    float wheel = 0.;
comp_72=`    
comp_73=`    a += box(uv5, float2(.7, .1), b);
comp_74=`    a += box(uv5, float2(0.75, .06), b);
comp_75=`    a += box(uv5 - float2(0., .2), float2(.5, .1), b);
comp_76=`    a += box(uv5 - float2(0., -.2), float2(.5, .1), b);
comp_77=`    
comp_78=`    a += rtr(float2(-uv5.x, uv5.y)-float2(.5, .1), float2(1., 1.), .2, b);
comp_79=`    a += rtr(float2(-uv5.x, -uv5.y)-float2(.5, .1), float2(1., 1.), .2, b);
comp_80=`    
comp_81=`    a += rtr(float2(uv5.x, uv5.y)-float2(.5, .1), float2(1., 1.), .2, b);
comp_82=`    a += rtr(float2(uv5.x, -uv5.y)-float2(.5, .1), float2(1., 1.), .2, b);
comp_83=`    
comp_84=`    a -= box(uv5-float2(-.25, .2), float2(.15, .1), b);
comp_85=`    a -= box(uv5-float2(.25, .2), float2(.15, .1), b);
comp_86=`    
comp_87=`    a = clamp(a, 0., 1.);
comp_88=`    
comp_89=`    wheel += circle(uv5-float2(.4,-.35), .15, .01);
comp_90=`    wheel += circle(uv5-float2(-.4,-.35), .15, .01);
comp_91=`    
comp_92=`    col = a * lerp(float3(.8, 0., 0.),float3(.8, .8, 0.), 
comp_93=`                  st(.2, .06, abs(uv5.y+.2)));
comp_94=`    col *= st(.01, .4, abs(uv5.y+.44));
comp_95=`    col *= st(.0, .2, abs(uv5.y-.38));
comp_96=`    	col *= st(.0, .9, abs(uv5.x+1.15));
comp_97=`    
comp_98=`    col *= 1.-wheel;
comp_99=`    
comp_100=`    return float4(col, a + wheel);
comp_101=`}
