comp_4=`
comp_5=`//BEGIN//
comp_6=`float2 R;
comp_7=`#define st(a, b, t) smoothstep(a, b, t)
comp_8=`
comp_9=`float2 rot(float2 p, float a){
comp_10=` 	return float2(p.x*cos(a) - p.y*sin(a), p.x*sin(a) + p.y*cos(a));   
comp_11=`}
comp_12=`
comp_13=`float height(float x){
comp_14=`    return sin(x*2.)*.15 + sin(x*3.)*.13 + sin(x*6.)*.02;
comp_15=`}
comp_16=`
comp_17=`
comp_18=`float aDx(float x){
comp_19=`    float h = .001;
comp_20=`    float y = height(x+h) - height(x);
comp_21=`    return atan2(y, h);
comp_22=`}
comp_23=`
comp_24=`float box(float2 uv1, float2 dim, float b)
comp_25=`{
comp_26=`    uv1 = abs(uv1);
comp_27=`    float bx = st(b, -b, uv1.y - dim.y);
comp_28=`    bx *= st(b, -b, uv1.x - dim.x );
comp_29=`    return bx;
comp_30=`}
comp_31=`
comp_32=`float circle(float2 uv2, float r, float b){
comp_33=` 	return st(r + b, r - b, length(uv2));   
comp_34=`}
comp_35=`float rtr(float2 uv3, float2 scale, float h,  float b){
comp_36=`    uv3.y -= h;
comp_37=`    uv3 *= scale;
comp_38=`    
comp_39=`    float tri = st(b, -b, dot(uv3, float2(1., 1.)));
comp_40=`    tri *= st(-b, b, uv3.y + h);
comp_41=`    tri *= st(-b, b, uv3.x);
comp_42=`    
comp_43=`    return tri;
comp_44=`}
