per_pixel_6=arc = cos(12*cos(2-abs(cos(2*rad-sin(12*ang)))));
per_pixel_7=cinc = above(sin(bass_att),1)*arc + below(sin(bass_att),0.9)*crude;
per_pixel_8=zoom = zoom + 0.1;
per_pixel_9=dx = dx + dx_r*cinc;
