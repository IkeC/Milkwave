per_frame_1=//           SOCS.12 -- Standard Operating Control System
per_frame_2=//                      By: Stirfriedramen
per_frame_3=// OUTPUTS:-------------------------------------------------------------
per_frame_4=//q32 -- volume        Rough signal indicating loudness (0 to 2-ish)
per_frame_5=//q31 -- volume_att    Attenuated loudness signal       (0 to 2-ish)
per_frame_6=//q30 -- vol_texture   How rough or smooth is volume (0=leg, 1=stec)
per_frame_7=//q29 -- bass_beat     boolian (1 or 0) 1 = sudden spike in bass
per_frame_8=//q28 -- bass_weight   % of total volume that is bass (0 to 1)
per_frame_9=//q27 -- bass_text     How rough or smooth is bass   (0=leg, 1=stec) 
per_frame_10=//q26 -- mid_beat      boolian (1 or 0) 1 = sudden spike in mid
per_frame_11=//q25 -- mid_weight    % of total volume that is mid  (0 to 1)
per_frame_12=//q24 -- mid_text      How rough or smooth is mid    (0=leg, 1=stec) 
per_frame_13=//q23 -- treb_beat     boolian (1 or 0) 1 = sudden spike in treb
per_frame_14=//q22 -- treb_weight   % of total volume that is treb (0 to 1)
per_frame_15=//q21 -- treb_tex      How rough or smooth is treb   (0=leg, 1=stec) 
per_frame_16=// CONTROLS:------------------------------------------------------------
per_frame_17=// These increase the senstivity of the beat detectors (0 to 1)
per_frame_18=b_beat_sens = .85;    m_beat_sens = .75;    t_beat_sens = .65;
per_frame_19=// Use these to set the sensitivity of the texture signals (0 .. 100)
per_frame_20=v_tex_sens = 10.0;  b_tex_sens = 50;  m_tex_sens = 50;  t_tex_sens = 5;
per_frame_21=// Set weights_smooth to 0 if you want to use the rougher signal
per_frame_22=weights_smooth = 1;
per_frame_23=// SOCS Code:-----------------------------------------------------------
per_frame_24=q32 = 1 / min((bass + mid + treb), 33);
per_frame_25=q31 = 1 / min((bass_att + mid_att + treb_att), 33);
per_frame_26=q29 = if((floor(max(bass - bass_att, 0) * (20 * b_beat_sens))),1,0);
per_frame_27=q28 = if((weights_smooth), bass_att * q31, bass * q32);
per_frame_28=b_tex = (b_tex * (100 - b_tex_sens) + abs(bass - bass_att) * b_tex_sens) * 0.01;
per_frame_29=q27 = min(b_tex * 1.333, 1);
per_frame_30=q26 = if((floor(max(mid  -  mid_att, 0) * (20 * m_beat_sens))),1,0);
per_frame_31=q25 = if((weights_smooth), mid_att * q31, mid * q32);
per_frame_32=m_tex = (m_tex * (100 - m_tex_sens) + abs(mid  -  mid_att) * m_tex_sens) * 0.01;
per_frame_33=q24 = min(m_tex * 1.166, 1);
per_frame_34=q23 = if((floor(max(treb - treb_att, 0) * (20 * t_beat_sens))),1,0);
per_frame_35=q22 = if((weights_smooth), treb_att * q31, treb * q32);
per_frame_36=t_tex = (t_tex * (100 - t_tex_sens) + abs(treb - treb_att) * t_tex_sens) * 0.01;
per_frame_37=q21 = min(t_tex * 1.166, 1);
per_frame_38=q32 = (bass + mid + treb);
per_frame_39=q31 = (bass_att + mid_att + treb_att);
per_frame_40=v_tex = (v_tex * (100 - v_tex_sens) + abs(q32 - q31) * v_tex_sens) * 0.01;
per_frame_41=q30 = min(v_tex, 1);
per_frame_42=q31 *= 0.060606;
per_frame_43=q32 *= 0.060606;
