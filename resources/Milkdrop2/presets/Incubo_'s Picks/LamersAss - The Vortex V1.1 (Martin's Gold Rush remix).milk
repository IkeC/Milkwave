per_frame_27=q7=0.4; //lev of reflection
per_frame_28=
per_frame_29=b_=b_*dec_m + (1+bass_att)*(1-dec_m);
per_frame_30=m_=m_*dec_m + (1+mid_att)*(1-dec_m);
per_frame_31=
per_frame_32=sp1+=b_*dt*(1+index2/8)/2;
per_frame_33=sp2+=m_*dt*(1+index/4);
